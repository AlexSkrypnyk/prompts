{
    "name": "laravel/prompts",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laravel\\Prompts\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/collections": "^10.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "pestphp/pest": "^2.3",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan-mockery": "^1.1"
    },
    "suggest": {
        "ext-pcntl": "Required for the spinner to be animated.",
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
